    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "tracking plot",
            "type": "python",
            "args": [
                "-hr", "8", // sim length 
            ],
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "src/utils/python/plot_tracking.py",
            "console": "integratedTerminal",
            "redirectOutput": false,
        },
    ]
}