{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) gnss-sdr",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/build/src/main/gnss-sdr",
            "args": [
                "-c", "work/gnss-sdr_GPS_L1_sbas.conf",
                // "-c", "work/gnss-sdr_GPS_L1_spain.conf",
                // "-c", "work/gnss-sdr_GPS_L1_darren.conf",
                "--signal_source", "work/geofix1_config17_30s_900mhz_take2.dat",
                // "--signal_source", "work/mini_0706_30s_4m_short_g50_trialA.dat",
                
                // DON'T FORGET TO CHANGE THE DUMP_DIR in the .CONF!!!

                "--log_dir", "data/sbas/geofix1_config17_30s_900mhz_take2"
                // "--log_dir", "data/sbas/mini_0706_30s_4m_short_g50_trialA"

            ],
            "stopAtEntry": false,
            "environment": [{
                "name": "LD_LIBRARY_PATH",
                "value": "/home/blmckinley/install/lib:$LD_LIBRARY_PATH"
            }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description":  "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "grab_data",
            "type": "python",
            "args": [
                "-n", "test_file.dat",  // file to save away
            ],
            "request": "launch",
            "cwd": "${workspaceFolder}/work",
            "program": "grab_data.py",
            "console": "integratedTerminal",
            "redirectOutput": false,
        },
        {
            "name": "gnssSdr Debug",
            "type": "python",
            "args": [
            ],
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "src/utils/python/gnssSdr.py",
            "console": "integratedTerminal",
            "redirectOutput": false,
        },
        
    ]
}